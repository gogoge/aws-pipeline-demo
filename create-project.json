{
    "name": "webb-build",
    "description": "webb-cli-build",
    "source": {
        "type": "CODEPIPELINE",
        "location": "https://git-codecommit.us-east-1.amazonaws.com/v1/repos/WebbGitRepo"
    },
    "artifacts": {
      "type": "CODEPIPELINE"
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/docker:1.12.1",
        "computeType": "BUILD_GENERAL1_SMALL",
        "environmentVariables": [
            {
            "name": "AWS_DEFAULT_REGION",
            "value": "us-east-1"
            },
            {
            "name": "AWS_ACCOUNT_ID",
            "value": "399485935277"
            },
            {
            "name": "IMAGE_REPO_NAME",
            "value": "webblu"
            },
            {
            "name": "IMAGE_TAG",
            "value": "latest"
            }
        ]
    },
    "serviceRole": "arn:aws:iam::399485935277:role/CodeBuildServiceRole",
    "timeoutInMinutes": 5
}
